--bag
function new_bag()
	b={content={}}
 return b
end

function fill_bag(b)
 b.content={}
 for x=1,3 do
 	for i=1,difficulty do
 		add(b.content,i)
 	end
 end
end

function from_bag(b)
	if #(b.content)==0 then
		fill_bag(b)
	end
	return del(b.content,rnd(b.content))
end
