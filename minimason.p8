--field
function new_field()
	return {}
end

function update_field(f)

end

function draw_field(f)
 rectfill(3*8,3*8,12*8,12*8,0)
	map(0,0,2*8,2*8,11,11)
end

function get_tile(x,y)
	return mget(x+1,y+1)
end

function set_tile(x,y,c)
	mset(x+1,y+1,c)
end

function field_empty(x,y)
	return mget(x+1,y+1)==0
end
