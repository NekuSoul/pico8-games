-- level generation
function genline()
  for x=0,7 do
    for y=0,11 do
      mset(x,y,mget(x,y+1))
    end
  end
  for x=0,7 do
    mset(x,12,rnd(10)+1)
  end
end

function genarrow()
  for i=0,15 do
    mset(18,i,mget(18,i+1))
  end
  mset(18,15,draw_arrow())
end
