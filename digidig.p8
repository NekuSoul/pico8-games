--arrow bag
function draw_arrow()
  if #arrowbag==0 then
    fill_bag()
  end
  printh(#arrowbag,"digidig")
  local val=rnd(arrowbag)
  return del(arrowbag,val)
end

function init_bag()
  arrowbag={11,11,12,12}
end

function fill_bag()
  arrowbag={}
  for i=0,3 do
    add(arrowbag,11)
    add(arrowbag,12)
  end
  add(arrowbag,13)
end
