-- digidig
-- by nekusoul
-- base loop
function _init()
  cartdata("nekusoul_ld48")
  started=false
  timer=0
  won=false
  lost=false
end

function init_game()
  init_anim()
  music(0)
  arrowbag={}
  fill_bag()
  lost=false
  won=false
  digit=0
  timer=0
  health=3
  buffer=0
  player_x=-3
  player_y=-1
  for i=1,16 do
    genline()
  end
  init_bag()
  for i=1,15 do
    genarrow()
  end
  mset(rnd(5)+1,12,25)
  mset(18,0,0)
  digit=-1
end

function _update60()
  if not won and not lost then
    timer+=1
  end
  player_input()
end

function _draw()
  if not started then
    draw_start()
    return
  end
  cls()
  shake_anim()
  --grass
  if player_x<-1 then
    mset(2,17,41) 
  else
    mset(2,17,44)  
  end
  map(0,16,0,0,16,2)
  --left wall
  map(10,0,0,16,4,16)
  --right wall
  map(14,0,96,16,4,16)
  --digits
  pal(7,1)
  map(0,0,32,16,8,player_y)
  pal()
  map(0,player_y,32,player_y*8+16,8,16-player_y)
  --arrows
  arrow_anim()
  if buffer==0 and player_y>-1 then
    print("❎",5,30)
  else
    spr(buffer,4,28)
  end
  map(18,1,16,40,1,5)
  --player
  local p_offset=0
  if player_y==-1 then
    p_offset=-1
  end
  local pose=(player_x+player_y)%2
  spr(23+pose,player_x*8+32,player_y*8+16+p_offset)
  --player arrows
  if player_y>-1 and player_x>0 then
    spr(39,player_x*8+24,player_y*8+16)
  end
  if player_x>-1 then
    spr(39,player_x*8+32,player_y*8+24)
  end
  if player_y>-1 and player_x<7 then
    spr(39,player_x*8+40,player_y*8+16)
  end
  --big digit
  digit_anim()
  --health
  map(19+health,0,104,48,1,3)
    if won then
    draw_win()
  end
  if lost then
    draw_lost()
  end
  --tent
  if player_x<-1 then
    spr(43,8,8)
    spr(40,16,8)
  end
  --tutorial inputs
  if player_y==-1 then
  local offset=(flr(timer/15))%2
    if player_x<0 then
    color(10)
      print("➡️",16,2-offset)
    else
    color(9)
      print("⬇️",player_x*8+32,2-offset)
    end
  end
  color()
end
