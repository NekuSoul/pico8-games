-- player input
function player_input()
  if not started then
    if btnp(❎) then
      started=true
      init_game()
    end
    return
  end
  if won or lost then
    if btnp(❎) then
      init_game()
      music(-1)
      started=false 
    end
    return
  end
  if btnp(⬇️) and player_x>-1 then
    if player_y==-1 then
      mset(player_x+4,17,48)
    elseif player_y==13 then
      return
    end
    player_move(player_x,player_y+1)
  end
  if btnp(⬅️) and (player_x>0 or (player_x>-3 and player_y==-1)) then
    player_move(player_x-1,player_y)
  end
  if btnp(➡️) and player_x<7 then
    player_move(player_x+1,player_y)
  end
  if btnp(❎) and player_y>-1 then
    swap_arrows()
    anim_to_arrow(mget(18,0))
    sfx(2)
  end
end

function player_move(x,y)
  local tdigit=mget(x,y)-1
  local allow=false
  local arrow=mget(18,0)
  anim_to_digit(digit,tdigit)
  if tdigit==-1 then
    allow=true
  elseif tdigit==24 then
    allow=true
    won=true
    music(-1)
    sfx(5)
  else
    if arrow==11 and tdigit>digit then
      allow=true
    end
    if arrow==12 and tdigit<digit then
      allow=true
    end
    if arrow==13 and tdigit==digit then
      allow=true
    end
  end
  if not allow and player_y>-1 then
    health-=1
    screenshake()
    sfx(3)
    if health==0 then
      lost=true
      music(-1)
      sfx(4)
    end
  end
  if tdigit!=-1 then
    genarrow()
    anim_to_arrow(mget(18,0))
  end
  player_x=x
  player_y=y
  if tdigit!=-1 then
    mset(x,y,0)
    digit=tdigit
    sfx(1)
  else
    sfx(0)
  end
end

function swap_arrows()
  if buffer==0 then
    buffer=mget(18,0)
    genarrow()
    return
  end
  local tbuffer=mget(18,0)
  mset(18,0,buffer)
  buffer=tbuffer
end
