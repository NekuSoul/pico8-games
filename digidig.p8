--endstates
function draw_win()
  map(19,3,36,52,7,4)
  color(10)
  print("…you won…",43,54)
  print("time: "..flr(timer/60),45,62)
  print("rank: "..get_rank(),45,70)
  print("❎ to restart",39,77)
  color()
end

function draw_lost()
  map(19,3,36,52,7,4)
  print("…you lost…",40,54)
  print("time: "..flr(timer/60),45,65)
  print("❎ to restart",39,77)
end

function get_rank()
  local t=flr(timer/60)
  t+=(3-health)*45
  if t>180 then
    return "f"
  elseif t>150 then
    return "e"
  elseif t>120 then
    return "d"
  elseif t>90 then
    return "c"
  elseif t>60 then
    return "b"
  elseif t>45 then
    return "a"
  elseif t>30 then
    return "s"
  elseif t>20 then
    return "ss"
  else
    return "sss"
  end
end
