--animations
function init_anim()
  d_time=60
  d_from=-1
  d_to=-1
  d_dir=false
  a_time=0
  s_time=0
end

function shake_anim()
  if s_time>0 then
    s_time-=1
  end
  local shake=s_time
  if (shake)%2==0 then
    shake=0
  end
  camera(shake/2,0)
end

function screenshake()
  s_time=8
end

function arrow_anim()
  clip(12,20,16,16)
  if a_time<15 then
    a_time+=1
  end
  local offsetx=0
  local offsety=(15-a_time)/2
  if a_arrow==12 then
    offsety=-offsety
  elseif a_arrow==13 then
    offsetx=offsety
    offsety=0
  end
  sspr(mget(18,0)*8,0,8,8,12+offsetx,20+offsety,16,16)
  clip()
end

function anim_to_arrow(arrow)
  a_time=0
  a_arrow=arrow
end

function digit_anim()
  clip(100,20,16,16)
  if d_time<30 then 
    d_time+=1
  end
  local offset=(30-d_time)/2
  local offsetb=offset-16
  if d_dir then
    offset=-offset
    offsetb=-offsetb
  end
  sspr((d_from+1)*8,0,8,8,100,20+offsetb,16,16)
  sspr((d_to+1)*8,0,8,8,100,20+offset,16,16)
  clip()
end

function anim_to_digit(from,to)
  if from==to or to==-1 then
    return
  end
  d_time=0
  d_from=from
  d_to=to
  d_dir=from>to
end
